file = _{SOI ~ program ~ EOI}
program = { sexpr* }
sexpr = {"(" ~ (sexpr | atom)* ~ ")"}
atom = _{ symbol | string }
symbol = @{ (ASCII_ALPHA | "-") ~ (ASCII_ALPHANUMERIC | "-")* }
string = ${ "\"" ~ inner ~ "\"" }
inner = @{ char* }
char = {
    !("\"" | "\\") ~ ANY
    | "\\" ~ ("\"" | "\\" | "/" | "f" | "n" | "r")
}
WHITESPACE = _{ " " | "\n" | "\r"}